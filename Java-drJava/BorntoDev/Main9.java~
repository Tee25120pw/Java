import java.util.Scanner;
 
public class Main9 {
 
    public static void main(String[] args) {
         
        Scanner scan = new Scanner(System.in);
         
        int Score = scan.nextInt();
         
        if(Score >= 80 && Score <= 100){
            System.out.print("A");
        }
        else if(Score >= 75){
            System.out.print("B+");
        }
        else if(Score >= 70){
            System.out.print("B");
        }
        else if(Score >= 65){
            System.out.print("C+");
        }
        else if(Score >= 60){
            System.out.print("C");
        }
        else if(Score >= 55){
            System.out.print("D+");
        }
        else if(Score >= 50){
            System.out.print("D");
        }
        else if(Score < 50 && Score >= 0){
            System.out.print("F");
        }
        else{
          System.out.print("Error : Value must be less than or equal to 100.");
        } 
    }
}