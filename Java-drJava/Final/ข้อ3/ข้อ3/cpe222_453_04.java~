public class cpe222_453_04{
  public static void main(String[] args){
    Motorcycle wave = new Motorcycle("Wave110i","Honda",2020,2);
    Chopper boulevard = new Chopper("Boulevard","Suzuki",1988,2);
    Car attrage = new Car("Attrage","Mitsubisho",2019,4,4);
    
    System.out.println(wave);
    System.out.println(boulevard);
    System.out.println(attrage);
  }
}