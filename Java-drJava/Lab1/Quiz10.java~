import javax.swing.*;
import java.awt.*;

public class Quiz10 {
  public static void main(String[] args) {
    Quiz10 f = new Quiz10();
    f.setTitle("Quiz10");
    f.setLocationRelativeTo(null);
    f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    f.setSize(300,350);
    f.setVisible(true);
  }}