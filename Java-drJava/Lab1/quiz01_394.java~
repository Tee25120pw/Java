import java.util.Scanner;
import javax.swing.JOptionPane;
public class quiz01_394 {
  public static void main(String[]args) {
    int b7,b6,b5,b4,b3,b2,b1,b0,x,y;
    Scanner sn = new Scanner(System.in);
    System.out.print("Please enter a binary value in the format b7 b6 b5 b4 b3 b2 b1 : ");
    b7 = sn.nextInt();
    b6 = sn.nextInt();
    b5 = sn.nextInt();
    b4 = sn.nextInt();
    b3 = sn.nextInt();
    b2 = sn.nextInt();
    b1 = sn.nextInt();
    b0 = sn.nextInt();
    x = (b7*8)+(b6*4)+(b5*2)+(b4*1);
    y = (b3*8)+(b2*4)+(b1*2)+(b0*1);
    JOptionPane.showMessageDialog(null, "Octal values = "+x+y+"","Message",JOptionPane.INFORMATION_MESSAGE);
  }
} 