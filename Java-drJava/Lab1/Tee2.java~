import java.util.Scanner;
public class Tee2{
  public static void main(String args []){
    int a,b,c,d,e,f,g,h,i,j,k,m;
    Scanner sn = new Scanner(System.in);
    System.out.print("Enter a year:");
    a = sn.nextInt();
    b = a%4;
    
    if (b == 0){
      switch(a){
      case 2008: System.out.printf("%d is a leap year. The Chinese zodiac of Rat",a);
      break;
      case 2012: System.out.printf("%d is a leap year. The Chinese zodiac of",a);
      break;
      case 2016: System.out.printf("%d is a leap year. The Chinese zodiac of Sheep",a);
      break;
      case 2020: System.out.printf("%d is a leap year. The Chinese zodiac of Pig",a);
      break;
      case 2024: System.out.printf("%d is a leap year. The Chinese zodiac of",a);
      break;
    }
    }
    else {
      switch(a){
      case 2009: System.out.printf("%d is not a leap year. The Chinese zodiac of Ox",a);
      break;
      case 2010: System.out.printf("%d is not a leap year. The Chinese zodiac of Tiger",a);
      break;
      case 2011: System.out.printf("%d is not a leap year. The Chinese zodiac of Rabbit",a);
      break;
      case 2013: System.out.printf("%d is not a leap year. The Chinese zodiac of Dragon",a);
      break;
      case 2014: System.out.printf("%d is not a leap year. The Chinese zodiac of snake",a);
      break;
      case 2015: System.out.printf("%d is not a leap year. The Chinese zodiac of horse",a);
      break;
      case 2017: System.out.printf("%d is not a leap year. The Chinese zodiac of Monkey",a);
      break;
      case 2018: System.out.printf("%d is not a leap year. The Chinese zodiac of Rooster",a);
      break;
      case 2019: System.out.printf("%d is not a leap year. The Chinese zodiac of Dog",a);
      break;      
      case 2021: System.out.printf("%d is not a leap year. The Chinese zodiac of Rat",a);
      break;
      case 2022: System.out.printf("%d is not a leap year. The Chinese zodiac of Ox",a);
      break;  
      case 2023: System.out.printf("%d is not a leap year. The Chinese zodiac of Tiger",a);
      break;
      
    }
  }
}
}